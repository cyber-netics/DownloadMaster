{
    "expo": {
        "name": "DownloadMaster",
        "slug": "download-master",
        "privacy": "public",
        "sdkVersion": "40.0.0",
        "platforms": [
            "ios",
            "android"
        ],
        "version": "1.2.0",
        "orientation": "portrait",
        "icon": "./assets/appstore.png",
        "splash": {
            "image": "./assets/Android.mp4",
            "resizeMode": "contain",
            "backgroundColor": "#ffffff"
        },
        "updates": {
            "fallbackToCacheTimeout": 0
        },
        "assetBundlePatterns": [
            "assets/**/*"
        ],
        "ios": {
            "bundleIdentifier": "com.download.master",
            "buildNumber": "1.2.0",
            "supportsTablet": true
        },
        "android": {
            "package": "com.download.master",
            "versionCode": 1,
            "adaptiveIcon": {
                "foregroundImage": "./assets/Android.mp4",
                "backgroundColor": "#FFFFFF"
            },
            "permissions": [
                "MEDIA_LIBRARY"
            ]
        }
    }
}